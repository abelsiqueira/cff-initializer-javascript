{
  "name": "cffinit",
  "version": "2.0.0",
  "description": "Web form to assist people in creating a CITATION.cff file for their projects.",
  "productName": "cffinit",
  "author": "Netherlands eScience Center",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue --ignore-pattern node_modules ./",
    "lint-fix": "eslint --ext .js,.ts,.vue --ignore-pattern node_modules ./ --fix",
    "test:unit:ui": "majestic",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.17.10",
    "@quasar/babel-preset-app": "^2.0.1",
    "@quasar/extras": "^1.0.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "deep-filter": "^1.0.2",
    "js-yaml": "^4.1.0",
    "kebabcase-keys": "^1.0.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0-alpha.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "majestic": "^1.7.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
